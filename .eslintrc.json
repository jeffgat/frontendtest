{
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-prettier",
    "@vue/typescript-eslint/recommended"
  ],
  "rules": {
    "vue/no-unused-vars": "warn"
  },
  "plugins": ["@typescript-eslint"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  }
}
